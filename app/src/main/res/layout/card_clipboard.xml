<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainFrame"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="horizontal">

    <LinearLayout
        android:id="@+id/lineInfoClip"
        android:layout_width="match_parent"
        android:layout_height="32dp"
        android:layout_gravity="start"
        android:layout_marginEnd="5dp"
        android:layout_toLeftOf="@+id/frameInageIconClipboard">

        <FrameLayout
            android:id="@+id/frameImageNum"
            android:layout_width="28dp"
            android:layout_height="25dp"
            android:layout_gravity="center_vertical|start"
            android:paddingLeft="6dp"
            android:visibility="visible">

            <ImageView
                android:id="@+id/imageTypeClipboard"
                android:layout_width="16dp"
                android:layout_height="14dp"
                android:layout_gravity="center_vertical|start"
                android:src="@drawable/icn_notes" />


            <TextView
                android:id="@+id/groupNumberClipboard"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="12.5dp"
                android:text="21"
                android:textColor="@color/orange"
                android:textSize="5.8dp"
                android:textStyle="bold"
                android:visibility="gone" />


            <TextView
                android:id="@+id/groupNumberClipboard_extracted"
                android:layout_width="wrap_content"
                android:layout_height="24dp"
                android:layout_marginLeft="15dp"
                android:layout_marginTop="1.5dp"
                android:text="2"
                android:textColor="@color/orange"
                android:textSize="5.8sp"
                android:textStyle="bold"
                android:visibility="visible" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/frameCircle"
            android:layout_width="22dp"
            android:layout_height="17dp"
            android:layout_gravity="center_vertical|start"
            android:layout_marginStart="6dp"
            android:paddingRight="6dp"
            android:visibility="gone">

            <ImageView
                android:id="@+id/imageTypeClipboardGroup"
                android:layout_width="16dp"
                android:layout_height="17dp"
                android:layout_gravity="center_vertical|start"
                android:src="@drawable/circle"

                android:visibility="gone" />

            <TextView
                android:id="@+id/groupCountClipboard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|center_vertical"
                android:text="2"

                android:textColor="@color/orange"
                android:textSize="10dp"

                android:textStyle="bold"
                android:visibility="gone" />

        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/clipboardValue"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical|start"
                android:layout_marginStart="6dp"
                android:layout_weight="1"
                android:ellipsize="end"
                android:fontFamily="sans-serif"
                android:maxLines="1"
                android:singleLine="true"
                android:text="https://www.facebook.com/call2me"
                android:textColor="@color/colorPrimaryDark"
                android:textSize="12sp"
                android:textStyle="normal" />

            <FrameLayout
                android:id="@+id/firstFindContact"
                android:layout_width="22dp"
                android:layout_height="32dp"
                android:layout_marginLeft="5dp"
                android:layout_toLeftOf="@+id/secondFindContact">

                <ImageView
                    android:id="@+id/companyPhotoIconFirst"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_gravity="right|center_vertical"

                    android:visibility="gone" />

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profileIconFirst"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center_vertical|right"

                    android:background="@android:color/transparent"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/profileIconInitialsFirst"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="2dp"
                    android:maxLength="2"
                    android:maxLines="1"
                    android:text="F"
                    android:textColor="@color/white"
                    android:textSize="6dp"
                    android:visibility="visible" />

            </FrameLayout>

            <FrameLayout
                android:id="@+id/secondFindContact"
                android:layout_width="20dp"
                android:layout_height="32dp"
                android:layout_marginLeft="5dp"
                android:layout_toLeftOf="@+id/frameCircleList"
                android:visibility="visible">

                <ImageView
                    android:id="@+id/companyPhotoIconSecond"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_gravity="right|center_vertical"

                    android:visibility="gone" />

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/profileIconSecond"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center_vertical|right"

                    android:background="@android:color/transparent"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/profileIconInitialsSecond"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_marginStart="1dp"
                    android:maxLength="2"
                    android:maxLines="1"
                    android:text="F"
                    android:textColor="@color/white"
                    android:textSize="6dp"
                    android:visibility="visible" />

            </FrameLayout>

            <FrameLayout
                android:id="@+id/frame_plane_history"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentEnd="true"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="7dp"

                android:visibility="gone">

                <ImageView
                    android:id="@+id/plane_history"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center_vertical|start"
                    android:src="@drawable/icn_ms_plane"
                    android:visibility="visible" />

            </FrameLayout>


            <FrameLayout
                android:id="@+id/frameCircleList"
                android:layout_width="18dp"
                android:layout_height="18dp"
                android:layout_alignParentEnd="true"
                android:layout_gravity="center_vertical"
                android:layout_marginStart="7dp"

                android:visibility="visible">

                <ImageView
                    android:id="@+id/imageTypeClipboardGroupList"
                    android:layout_width="18dp"
                    android:layout_height="18dp"
                    android:layout_gravity="center_vertical|start"
                    android:src="@drawable/circle"

                    android:visibility="visible" />


                <TextView
                    android:id="@+id/contactNumbers"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:layout_marginEnd="0dp"
                    android:text="4"
                    android:textColor="@color/orange"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    android:visibility="visible" />

            </FrameLayout>

        </LinearLayout>

    </LinearLayout>

    <FrameLayout
        android:id="@+id/frameInageIconClipboard"
        android:layout_width="25dp"
        android:layout_height="32dp"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="33dp">

        <ImageView
            android:id="@+id/companyPhotoIcon"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_gravity="right|center_vertical"
            android:layout_marginRight="3dp"
            android:visibility="gone" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profileIconAvatar"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="3dp"
            android:background="@android:color/transparent"
            android:visibility="gone" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profileIconSearch"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_gravity="center_vertical|right"
            android:layout_marginRight="3dp"
            android:background="@android:color/transparent"
            android:src="@drawable/sand_clock"


            android:visibility="gone" />

        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/profileIconArrow"
            android:layout_width="13dp"
            android:layout_height="13dp"
            android:layout_gravity="center_vertical|right"

            android:layout_marginRight="6dp"
            android:background="@android:color/transparent"
            android:src="@drawable/arr_down"
            android:visibility="gone" />

        <TextView
            android:id="@+id/profileIconInitials"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|center_horizontal"
            android:maxLength="2"
            android:maxLines="1"
            android:text="F"
            android:textColor="@color/white"
            android:textSize="6dp"
            android:visibility="gone" />

        <FrameLayout
            android:id="@+id/frameCircleSearch"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_gravity="center_horizontal|center_vertical"
            android:visibility="visible">

            <FrameLayout
                android:id="@+id/search_note"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/circle_search"
                    android:layout_width="18dp"
                    android:layout_height="match_parent"
                    android:src="@drawable/circle_search"
                    android:visibility="visible" />

                <!--suppress AndroidDomInspection -->
                <TextView
                    android:id="@+id/number_search"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal|center_vertical"
                    android:layout_marginEnd="0dp"
                    android:text="?"
                    android:textColor="@color/orange"
                    android:textSize="10dp"
                    android:textStyle="bold"
                    android:visibility="visible" />

            </FrameLayout>


            <FrameLayout
                android:id="@+id/frameSocialsSearch"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <ImageView
                    android:layout_width="18dp"
                    android:layout_height="match_parent"
                    android:padding="1dp"
                    android:src="@drawable/icn_extract_social"
                    android:visibility="visible" />

            </FrameLayout>


        </FrameLayout>

    </FrameLayout>

    <RelativeLayout
        android:id="@+id/checkFrameClip"
        android:layout_width="27dp"
        android:layout_height="32dp"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="5dp">

        <!-- <ImageView
             android:id="@+id/circleImage"
             android:layout_width="16dp"
             android:layout_height="16dp"
             android:layout_gravity="center_vertical|start"
             android:src="@drawable/icn_clipboard_plus_card" />-->

        <!--<TextView
            android:id="@+id/timeClipboard"
            android:layout_marginStart="5dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="12sp"
            android:fontFamily="sans-serif-light"
            android:textStyle="normal"
            android:textColor="@color/colorPrimaryDark"
            android:gravity="end"
            android:layout_gravity="center_vertical"
            android:text="@string/_21_00"/>-->

        <CheckBox
            android:id="@+id/checkClip"
            style="@style/CheckBoxStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="6dp"
            android:checked="false"
            android:clickable="false" />
    </RelativeLayout>


</RelativeLayout>

